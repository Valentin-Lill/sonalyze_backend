# Sonalyze Backend Environment Configuration
# Copy this file to .env and adjust values as needed

# Authentication token for internal service communication
INTERNAL_AUTH_TOKEN=sonalyze_internal_token_change_me

# PostgreSQL configuration
POSTGRES_USER=sonalyze
POSTGRES_PASSWORD=sonalyze_secret_change_me
POSTGRES_DB=sonalyze

# Gateway settings
RATE_LIMIT_RPS=10.0
RATE_LIMIT_BURST=20
MAX_MESSAGE_BYTES=65536
HTTP_TIMEOUT_SECONDS=30.0

# Measurement settings
MEASUREMENT_MAX_UPLOAD_MB=50

# Storage settings
LOG_LEVEL=INFO
RUN_MIGRATIONS=true
